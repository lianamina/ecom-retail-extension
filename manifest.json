{   
    "manifest_version": 3,
    "name": "Showroom",
    "description": "Convergent project",
    "version": "3.0",
    
    "content_security_policy": {
      "extension_pages": "script-src 'self' https://www.tiktok.com; object-src 'self'",
      "sandbox": "sandbox allow-scripts; script-src 'self' https://tiktok.com",
      "sandbox": {"pages": ["index.html","assets/Static/tiktok.html"]}
    },

    "icons": {
      
    },

    "action": {
      "default_icon": "icon.png",
      "default_popup": "html/popup.html"
    },

    "background": {
      "service_worker": "js/background.js"
    },

    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": [ "js/content.js" ]
      }
     ],

    "options_page": "html/options.html",

    "permissions": [
      "tabs",
      "activeTab",
      "scripting"
    ],

    "host_permissions": [
      "<all_urls>"
    ]
  }
